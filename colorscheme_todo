# Terminal
Replace $HOME/lib/scripts/wm/dwm/xresources-colors/current_theme with $colorscheme

# Nvim 
Replace $HOME/.local/share/nvim/lazy/base46/lua/base46/themes/current_theme.lua with $colorscheme

# BG
replace $HOME/lib/images/wallpapers/current_theme with $HOME/lib/images/wallpapers/colorscheme/$colorscheme

# Spicetify
spicetify config color_scheme rosepine && spicetify apply

#DWM 
replace current_theme.h with $colorscheme.h


